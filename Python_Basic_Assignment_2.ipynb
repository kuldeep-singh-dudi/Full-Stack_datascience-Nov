   "source": [
    "spam = int(input(\"enter the value: \"))\n",
    "if spam ==1:\n",
    "    print('Hello')\n",
    "elif spam==2:\n",
    "    print('Howdy')\n",
    "else :\n",
    "    print('Greetings!')"
